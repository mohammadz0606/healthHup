

<%- include("../partials/dashboard/header.ejs") %>


<!-- partial -->
<div class="container-fluid page-body-wrapper">
  <!-- partial:partials/_sidebar.html -->
  <nav class="sidebar sidebar-offcanvas" id="sidebar">
    <ul class="nav">

      <%-include("../partials/dashboard/navList.ejs") %>
      
    </ul>
  </nav>
  <!-- partial -->
  <div class="main-panel">
    <div class="content-wrapper">
      <div class="row">
        <div class="col-md-12 grid-margin">
          <div class="d-flex justify-content-between align-items-center">
            <h2 class="font-weight-bold mb-0">Feedbacks</h2>
            
          </div>
        </div>
        <div class="col-12 d-flex align-items-center justify-content-between">
            <p class="lead"><%= feedbacks.length %> feedbacks Found</p>
            <a href="/dashboard/manage/feedbacks" class="btn btn-light">
                <svg style="margin-right: 10px;" xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-arrow-clockwise" viewBox="0 0 16 16">
                    <path fill-rule="evenodd" d="M8 3a5 5 0 1 0 4.546 2.914.5.5 0 0 1 .908-.417A6 6 0 1 1 8 2z"/>
                    <path d="M8 4.466V.534a.25.25 0 0 1 .41-.192l2.36 1.966c.12.1.12.284 0 .384L8.41 4.658A.25.25 0 0 1 8 4.466"/>
                  </svg>
                Reload Feedbacks
            </a>
        </div>
        <div class="col-12 mt-5">
            <div class="row">
                <% feedbacks.forEach((feedback) => { %>
                  <div class="col-4 mx-2 bg-light p-4 mt-3" style="border-radius: 15px;">
                       

                  

                      <p class="mb-0" style="font-weight: bold; text-transform: uppercase;">STARS</p>
                      <p class=" mb-0"><%= feedback.stars%></p>
                      <br>

                      <p class="mb-0" style="font-weight: bold; text-transform: uppercase;">COMMENT</p>
                      <p class=" mb-0"><%= feedback.comment %></p>
                      <br>
                      <br>
                      <a class="btn btn-danger text-light" href="/dashboard/manage/delete/feedback/<%= feedback._id %>">
                        <svg style="margin-right:8px" xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-trash3" viewBox="0 0 16 16">
                        <path d="M6.5 1h3a.5.5 0 0 1 .5.5v1H6v-1a.5.5 0 0 1 .5-.5M11 2.5v-1A1.5 1.5 0 0 0 9.5 0h-3A1.5 1.5 0 0 0 5 1.5v1H1.5a.5.5 0 0 0 0 1h.538l.853 10.66A2 2 0 0 0 4.885 16h6.23a2 2 0 0 0 1.994-1.84l.853-10.66h.538a.5.5 0 0 0 0-1zm1.958 1-.846 10.58a1 1 0 0 1-.997.92h-6.23a1 1 0 0 1-.997-.92L3.042 3.5zm-7.487 1a.5.5 0 0 1 .528.47l.5 8.5a.5.5 0 0 1-.998.06L5 5.03a.5.5 0 0 1 .47-.53Zm5.058 0a.5.5 0 0 1 .47.53l-.5 8.5a.5.5 0 1 1-.998-.06l.5-8.5a.5.5 0 0 1 .528-.47M8 4.5a.5.5 0 0 1 .5.5v8.5a.5.5 0 0 1-1 0V5a.5.5 0 0 1 .5-.5"/>
                      </svg>
                      Delete</a>
                  </div>
                <% }) %>
                    
                    
                   
                   
                
                
                
            </div>
        </div>
     </div>
    </div>
    
      
    </div>
    
</div>



  
<%- include("../partials/dashboard/footer.ejs") %>



